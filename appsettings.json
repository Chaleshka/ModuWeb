{
  "BaseApiPath": "api/",
  "BaseDbPath": "db/",
  "MaxRequestBodySize": 50, //In MB
  "UseHttps": true,
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:5001",
        "Certificate": { //If you dont need Certificate, just delete `"Certificate":{...}`
          "Path": "cert/cert.pem", //Path to your cert
          "KeyPath": "cert/key.pem" //Path to your key for cert
        }
      }
    }
  },
  "LoadOrder": [ //The order of the modules that will be loaded first and in the following order

  ]
}
